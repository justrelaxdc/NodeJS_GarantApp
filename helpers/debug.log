let su_user = "";

class OrderClass {
    constructor(id) {
        this.id_order = id;
        this.state = 0;
        this.job = null;
      }

    set_state() {
        if (this.is_deleted) {
            this.state = 6;            // deleted
        } else if (this.is_active && !this.is_white) {
            if (this.is_owner_accept == '0') {
                this.state = 4;        // 4.owner_approval_waiting
            } else {
                this.state = 2;        // moderation_waiting
            }
        } else if (this.is_active && this.is_white) {
            this.state = 3;            // working
        } else if (!this.is_active && this.is_white) {
            this.state = 5;            // stopped
        } else if (!this.is_active && !this.is_white) {
            this.state = 1;            // preparation
        }
    }

    set_job() {
        this.job = $("#job_" + this.id_order);
    }

    html_subsitute(html) {
        let word_list = ['id_order', 'id_notice'];
        for (let word of word_list) {
            let rExp = new RegExp('=\\|'+word+'\\|=', "g");
            html = html.replace(rExp, this[word]);
        }
        return html;
    }

    get_inner_substatus() {
        let state = this.state;
        if (state == 4 && this.my_order) {
            let comment = $(document).find("#order_substatus_comment4");
            return this.html_subsitute(comment[0].outerHTML);
        }
    }

    show_comment_all() {
        console.log('show_comment_all')
        if (! this.job) this.set_job();
        let osp = this.job.find('#job_desc_'+this.id_order);  // order sub status panel
        if (osp) {
            let div = $(this.get_inner_substatus()).insertBefore(osp).show();
            let attr = div.attr('id');
            div.attr('id', attr + '_' + this.id_order);
            if (this.notice_order_desc_changed) {
                div.find('.order_desc_changed').show();
            }
            if (this.notice_tender_desc_changed) {
                div.find('.tender_desc_changed').show();
            }
            if (this.notice_order_title_changed) {
                div.find('.order_title_changed').show();
            }
        }
    }

    hide_comment() {
        if (this.state == 4 && ! this.su_user) {
            let comment4 = this.job.find("#order_substatus_comment4_" + this.id_order);
            comment4.slideUp(500, function() {comment4.remove();});
        }
    }
    
    comment(flag, details) {
        if (flag != 'good' && flag != 'bad') return;
        let comment4 = this.job.find("#order_substatus_comment4_" + this.id_order);
        if (details) {
            flag = flag + '.' + details;
        }
        comment4.find(".status_" + flag).slideDown();
    }

    waiting(on) {
        if (on) {
            if (this.state == 4 && ! this.su_user) {
                let comment4 = this.job.find("#order_substatus_comment4_" + this.id_order);
                let message = comment4.find(".comment");
                message.slideUp(500, function() {message.remove();});
                this.loading = comment4.find(".order_appr_loading").show();
            }
        } else {
            let obj = this.loading;
            obj.slideUp(500, function() {obj.remove();});
        }
    }
}

function change_order_state(ev) {
    let Order = orders[this.getAttribute("dataid")];
    Order.waiting(true);
    let a_url = '/' + this.id.replace(/_/g, '/') + '/';
    let stop_flag = this.id.endsWith('stop');
    let a_data = '';

    if (stop_flag) {
        Order.waiting(false);
        Order.comment('good', 'order_stopped');
        let button = Order.job.find("#order_stop_submit_" + Order.id_order);
        order_ajax_prepare(ev, button[0]);
    } else {
            $.ajax({
                url:      a_url,
                type:     "GET",
                data:     a_data,
                dataType: "html",
                success:  function(data) {
                        Order.waiting(false);
                        Order.comment('good', 'order_approved');
                    },
                error:    
                    function() { 
                        Order.waiting(false);
                        Order.comment('bad');
                    }
            });
    }
}

