<script>
    $('.single-slide').slick({
        dots: true,
        infinite: true,
        slidesToShow: 3,
        slidesToScroll: 1,
        variableWidth: true,
        variableHeight: true,
        centerMode: true,
        focusOnSelect: true
    });

    function contentFill(that) {
        var obj = $(".infoModalBox .data_incoming");
        obj.html(that.html());
        obj.find('.etc').hide();
        obj.find('.full_text').show();
    }

    $('.single-slide .slider_item').on('click', function(){
        contentFill($(this));
        $(".infoModalBox").modal('show');
    });

    var lastOrdersData = [];
    var rotator = {
        start   :0,
        size    :20,
        frames  :5,
        currentPos : 0,
        nextPos : function(){
            if (this.frames < this.size) {
                this.currentPos = this.frames;
                return {from:this.start , to : this.frames}
            } else {
                this.currentPos = this.size;
                return {from:this.start , to:this.size}
            }
        }
    };

    function renderBlock(data) {
        pos = rotator.nextPos();
        $('.pricesItems .row').html('');
        $('.pricesItemsMobile').html('');
        for (var i = pos.from; i < pos.to; i++ )
        {
            var obj = data[i];

            var template = compileTemplate(obj);
            var mobileTemplate = compileMobTemplate(obj);

            $('.pricesItems').append(template);
            $('.pricesItemsMobile').append(mobileTemplate);
        }

    }


    function titleFormat(type, phrase, publication ) {

        let res = phrase;
        if (
            type.indexOf("TEXT / Комментарии и консультации")<0 &&
            type.indexOf("SOCIAL / Публикации в соцсетях")<0 &&
            type.indexOf("ADV / Вирусные комментарии и отзывы")<0 &&
            type.indexOf("ADV / Публикация на ресурсе исполнителя")<0
        ) {
            res = res + publication;
        }

        return res;

    }



    function firstLower (str) {
        if (str  && str.length > 1) {
            return str.charAt(0).toLowerCase() + str.substr(1);
        } else return str
    }

    function compileTemplate(obj) {
        var template = $('.itemTemplate .row').clone();
        var title = titleFormat(obj.type, obj.phrase , obj.publication);
        template.find('.ava a').attr('href','/profile/' + obj.master + '/master');
        template.find('.ava img').attr('src','/pic/' + obj.master + '.jpg').attr('alt',obj.master);
        template.find('.item_title > div').append( title );
        template.find('a.item_title').attr('href','/job/order/' + obj.id);
        template.find('a.item_title').attr('id','titleItemId' + obj.id);
        //template.find('.item_title > div').append(obj.title);
        //template.find('.item_title > div  div').html( (obj.category != 'Без категории') ? obj.category : '' );
        template.find('.symbols_count span').html(obj.simbols + ' зн.');
        template.find('.hours_count span').html(obj.time);
        template.find('.price_count .green_button span').html(obj.price);
        template.find('a.green_button ').attr('href','/job/order/' + obj.id);
        template.find('a.green_button ').attr('id','itemId' + obj.id);
        return template
    }



    function compileMobTemplate(obj) {
        var mobileTemplate = $('.mobileTemplate .row').clone();
        var title = titleFormat(obj.type, obj.phrase , obj.publication);

        //mobileTemplate.find('.item_title_m div').html( (obj.category != 'Без категории') ? obj.category : '' );
        mobileTemplate.find('.ava_m a').attr('href','/profile/' + obj.master + '/master');
        mobileTemplate.find('.ava_m img').attr('src','/pic/' + obj.master + '.jpg').attr('alt',obj.master);
        mobileTemplate.find('.item_title_m ').append(title);
        mobileTemplate.find('.item_title_m ').attr('href','/job/order/' + obj.id);
        mobileTemplate.find('.item_title_m ').attr('id','itemTitleIdMob' + obj.id);
        mobileTemplate.find('.symbols_hours_m').html(obj.simbols + ' зн. / ' + obj.time);
        mobileTemplate.find('.price_count_m .green_button:not(.mob) span').html(obj.price);
        mobileTemplate.find('a.green_button ').attr('href','/job/order/' + obj.id);
        mobileTemplate.find('a.green_button ').attr('id','itemIdMob' + obj.id);
        return mobileTemplate
    }

    function addOneItem(data) {
        if (rotator.size == rotator.currentPos) { return undefined; }

        if (rotator.currentPos + 1 < rotator.size ) {
            rotator.currentPos += 1;
            var obj = data[rotator.currentPos];

            var template = compileTemplate(obj);
            template.css({'display':'none'});
            var mobileTemplate = compileMobTemplate(obj);
            mobileTemplate.css({'display':'none'});

            $('.pricesItems').prepend(template);
            $('.pricesItems .row:first-child').slideDown('slow');
            $('.pricesItems .row:last-child').slideUp('slow', function() { $(this).remove(); });
            $('.pricesItemsMobile').prepend(mobileTemplate);
            $('.pricesItemsMobile .row:first-child').slideDown('slow');
            $('.pricesItemsMobile .row:last-child').slideUp('slow', function() { $(this).remove(); })
        } else {
            rotator.currentPos = 0;
        }
    }

    function loggedLandingLinks() {
        var selectors = '#lets_work, #ten_more_works, #one_click_registration, #arrow_right_join, #show_all_tasks, #show_all_tasks_mobile, #please_register, #registration_link, #second_arrow_right_register, #starts_work, #lets_work_it, #internet_work, #reg_by_click, #pay_for_works, #search_for_work, #works_for_pay, #lets_start_new_job';
        $(selectors).attr("href", "/job/find/").off();
        $('#arrow_right_blog').attr("href", "/blog/post/feedback/").off();
    }

    $(function() {

        

        $.ajax({
            url: '/jsnc/order/?method=last_orders&limit=20',
            type: "POST",
            datatype:"JSON",
            success: function(data) {
                data = JSON.parse(data);
                if (data != undefined && 'result' in data  && data.result.length > 0   )
                {

                    var orders = ('orders' in data.result[0] ) ? data.result[0].orders : [];
                    lastOrdersData = orders ;

                    lastOrdersData = lastOrdersData.filter((obj)=>{
                        if (obj.type == '')
                        {return false}
                        else {return true}
                    });

                    lastOrdersData.forEach((obj)=>{
                        var priceData = obj.price.split(' ');
                        if(priceData && priceData.length == 2) {
                            var priceNumber = parseFloat(priceData[0]).toFixed(2);
                            obj.price = priceNumber + ' ' + priceData[1];
                        }
                    });

                    rotator.size = orders.length;
                    $('.non_mobile_prices').css({'visibility':'visible'});
                    $('.mobile_prices').css({'visibility':'visible'});
                    $('.pricesPreloader').remove();

                }

                renderBlock(lastOrdersData)
            } });
    });


    window.onscroll = function() {
        var scrolled = window.pageYOffset || document.documentElement.scrollTop;
        if (scrolled > 2100) {
            window.onscroll = function() {};//сброс проверок
            if (lastOrdersData.length>0) {
                addOneItem(lastOrdersData);
            }

            setInterval(function(){
                if (lastOrdersData.length>0) {
                    addOneItem(lastOrdersData);
                }
            } , 5000);
        }
    };

    $(window).on('resize', function () {
        verticalCalc();
    });


    function verticalCalc() {
        var heightArrow = $('.arrow_right').closest('.container').css('height');

        if (heightArrow != undefined )  {
            heightArrow = parseFloat(heightArrow,10);
            $('.arrow_right').css({'top': (heightArrow/2 - 30) + 'px'});
        }
    }

    function redirAfter(path)
    {
        window.newRedir = path;
    }
</script>

